{
    "name": "litebase/litebase-php",
    "version": "1.0.0",
    "description": "Litebase Server OpenAPI specification",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "MIT",
    "authors": [
        {
            "name": "Litebase",
            "homepage": "https://litebase.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Litebase\\Generated\\" : "./src/Generated/" }
    },
    "autoload-dev": {
        "psr-4": { "Litebase\\Generated\\Test\\" : "test/" }
    }
}
